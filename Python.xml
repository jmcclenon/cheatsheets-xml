    <section title="Date Formatting (strfime and strptime)">
        <item>
            <code>%a</code>
            <text pos="right">Abbreviated weekday (Sun)</text>
        </item>
        <item>
            <code>%A</code>
            <text pos="right">Weekday (Sunday)</text>
        </item>
        <item>
            <code>%b</code>
            <text pos="right">Abbreviated month name (Jan)</text>
        </item>
        <item>
            <code>%B</code>
